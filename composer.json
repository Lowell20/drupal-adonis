{
    "minimum-stability": "alpha",
    "name": "craychee/adonis",
    "license": "MIT",
    "description": "The prettiest Drupal workflow.",
    "repositories": [
        {
        "type": "composer",
        "url": "http://packagist.drupal-composer.org/"
    }
    ],
    "require": {
        "behat/behat": "3.0.*@dev",
        "composer/composer": "~1.0@alpha",
        "drupal/admin_menu": "~7.3@RC",
        "drupal/drupal": "7.*",
        "drupal/module_filter": "~7.2@alpha",
        "drupal/tangler": "*",
        "drush/drush": "6.*",
        "winmillwill/settings_compile": "~2.1.1"
    },
    "require-dev": {
        "behat/behat": "3.0.*@dev",
        "drupal/drupal-extension": "3.0.*",
        "phpspec/phpspec": "~2.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": {
            "": "src"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "": "spec"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bin/wrapper"
        ],
        "post-update-cmd": [
            "bin/wrapper"
        ]
    },
    "type": "project"
}
